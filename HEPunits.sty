\NeedsTeXFormat{LaTeX2e}
\ProvidesPackage{HEPunits}[2003/05/02 v1.0 Units for High Energy Physics]

\RequirePackage[amssymb]{SIunits}

\renewcommand{\unit}[1]{\ensuremath{\@qsk\period@active{#1}}}

\addunit{\eV}{\electronvolt}
\addunit{\KeV}{\kilo\eV}
\addunit{\MeV}{\mega\eV}
\addunit{\GeV}{\giga\eV}
\addunit{\TeV}{\tera\eV}
\addunit{\PeV}{\peta\eV}
\addunit{\EeV}{\exa\eV}
\addunit{\yyear}{yr}

